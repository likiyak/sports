{
  "version": 2,
  "builds": [
    {
      "src": "api/collector.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/collect",
      "destination": "/api/collector"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
